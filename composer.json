{
  "name": "lionheart/blog-editor-addon",
  "description": "This is a WordPress plugin for the editor - it helps bloggers edit all their blog content right from their blog page.",
  "type": "project",
  "keywords": [
    "wordpress",
    "wp",
    "plugin",
    "editor"
  ],
  "homepage": "https://lionheartdevs.com",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Chris Norman",
      "email": "chris@lionheartdevs.com"
    }
  ],
  "require": {
    "php": "^7.0"
  },

  "autoload": {
    "classmap": [],
    "files": [
      "src/contents.php",
      "src/admin/enable-editor.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "vendor-dir": "assets/vendor"
  }
}
